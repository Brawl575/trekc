_G.WebhookDataCode = [[
    -- –®–∞–±–ª–æ–Ω –¥–ª—è embeds
    _G.embedTemplate = {
        embeds = {{
            title = "Nameless Pet Notifier",
            color = 5814783,
            description = "",
            fields = {
                {name = "üë• Players:", value = "", inline = true},
                {name = "üîó Server Link:", value = "", inline = false},
                {name = "üì± Job-ID (Mobile):", value = "", inline = false},
                {name = "üíª Job-ID (PC):", value = "", inline = false},
                {name = "üì≤ Join:", value = "", inline = false}
            }
        }}
    }

    -- –§—É—Ç–µ—Ä –¥–ª—è –≤–µ–±—Ö—É–∫–∞
    _G.embedFooter = {text = "Buy premium for 3M+ notifier!"}

    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≥–ª—É–±–æ–∫–æ–π –∫–æ–ø–∏–∏ —Ç–∞–±–ª–∏—Ü—ã
    local function deepCopy(orig)
        local copy = {}
        for k, v in pairs(orig) do
            if type(v) == "table" then
                copy[k] = deepCopy(v)
            else
                copy[k] = v
            end
        end
        return copy
    end

    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ñ–æ—Ä–º–∞—Ç–∏—Ä–æ–≤–∞–Ω–∏—è –¥–∞–Ω–Ω—ã—Ö –º–æ–¥–µ–ª–µ–π –¥–ª—è JSON
    _G.formatWebhookData = function(models, placeId, jobId, playerCount, maxPlayers)
        -- —Å–æ–∑–¥–∞—ë–º –∫–æ–ø–∏—é —à–∞–±–ª–æ–Ω–∞
        local data = deepCopy(_G.embedTemplate)

        local browserLink = "https://nameless-289z.onrender.com/join.html?placeId=" .. tostring(placeId) .. "&jobId=" .. tostring(jobId)
        local joinScript = 'game:GetService("TeleportService"):TeleportToPlaceInstance(' .. tostring(placeId) .. ',"' .. tostring(jobId) .. '",game.Players.LocalPlayer)'

        -- –§–æ—Ä–º–∏—Ä—É–µ–º –æ–ø–∏—Å–∞–Ω–∏–µ —Å –¥–∞–Ω–Ω—ã–º–∏ –º–æ–¥–µ–ª–µ–π
        local descriptionLines = {}
        for entry in models:gmatch("[^,]+") do
            local trimmed = entry:match("^%s*(.-)%s*$")
            local parts = {}
            for part in trimmed:gmatch("([^|]+)") do
                table.insert(parts, part:match("^%s*(.-)%s*$"))
            end
            if #parts >= 4 then
                local displayName, generation, mutation, rarity = parts[1], parts[2], parts[3], parts[4]
                table.insert(descriptionLines, string.format("**%s** (%s, %s, %s)", 
                    displayName or "Unknown", 
                    rarity or "None", 
                    generation or "Unknown", 
                    mutation or "Default"))
            end
        end
        data.embeds[1].description = table.concat(descriptionLines, "\n")

        -- –û–±–Ω–æ–≤–ª—è–µ–º –ø–æ–ª—è
        data.embeds[1].fields[1].value = tostring(playerCount) .. "/" .. tostring(maxPlayers)
        data.embeds[1].fields[2].value = "[Join Server](" .. browserLink .. ")"
        data.embeds[1].fields[3].value = tostring(jobId)
        data.embeds[1].fields[4].value = tostring(jobId)
        data.embeds[1].fields[5].value = "`" .. joinScript .. "`"

        -- –î–æ–±–∞–≤–ª—è–µ–º —Ñ—É—Ç–µ—Ä
        data.embeds[1].footer = _G.embedFooter

        return data
    end
]]
